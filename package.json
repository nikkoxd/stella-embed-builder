{
  "name": "stella-embed-builder",
  "version": "1.0.0",
  "description": "Embed generator for nikkoxd/stella",
  "repository": "https://github.com/nikkoxd/stella-embed-builder.git",
  "author": "nikko <35370166+nikkoxd@users.noreply.github.com>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "5.^3.2",
    "bootstrap-icons": "1.^11.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "sass": "^1.70.0",
    "sirv-cli": "^2.0.2",
    "stylelint": "^16.2.1",
    "stylelint-config-twbs-bootstrap": "^14.0.0"
  },
  "scripts": {
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules scss/:css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "server": "sirv --dev --no-clear --port 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm-run-all css-lint css"
  }
}
